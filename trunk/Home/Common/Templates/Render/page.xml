<?xml version="1.0" encoding="UTF-8"?>
<renderTemplate>
	<html xmlns="http://www.w3.org/1999/xhtml">
		<head>
			<title>$PAGE_TITLE$</title>
			<meta name="generator" content="HomePortals Portal Framework" />
			<meta http-equiv="Content-Type" content="text/html; charset=iso-8859-1" />
			<meta http-equiv="Expires" content="0" />
			<meta http-equiv="Pragma" CONTENT="no-cache" />
			<meta http-equiv="CACHE-CONTROL" CONTENT="NO-CACHE" />
			<meta name="expires" content="0" />
			$PAGE_HTMLHEAD$
		</head>

		<body onLoad="$PAGE_ONLOAD$">
			<div id="h_main">
				<!-- Custom headers -->
				$PAGE_CUSTOMSECTION["HEADER"]$
				
				<!-- Header Sections -->
				$PAGE_LAYOUTSECTION["HEADER"]["DIV"]$
	
				<!-- Column sections -->
				<div style="##height:1%;">
					<table border="0" cellspacing="0" cellpadding="0" width="100%" id="h_body_main">
						<tr>
							$PAGE_LAYOUTSECTION["COLUMN"]["TD"]$
						</tr>
					</table>
				</div>
	
				<!-- Footer sections -->
				$PAGE_LAYOUTSECTION["FOOTER"]["DIV"]$
			</div>

			<!-- Custom footers -->
			<div id="h_footer">
				$PAGE_CUSTOMSECTION["FOOTER"]$
			</div>
		</body>
	</html>
</renderTemplate>

<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="/Home/pageLoader.xsl"?><Page>
	<title>News Reader</title>
	
	<stylesheet href="/Home/Skins/hp/hp.css"/>
	
	<layout>
		<location class="column_small" name="left" type="column"/>
		<location class="column" name="right" type="column"/>
	</layout>
	
	<modules>
		<module CONTAINER="true" DISPLAY="normal" OUTPUT="true" SHOWPRINT="true" STYLE="" followLink="false" 
			id="bookmarks22" location="left" name="Bookmarks/bookmarks" title="News Feeds" 
			url="/Home/resourceLibrary/Pages/News Reader/NewsFeeds.xml"/>
		<module CONTAINER="true" DISPLAY="normal" OUTPUT="true" SHOWPRINT="true" STYLE="" followLink="false" 
			id="bookmarks21" location="left" name="Bookmarks/bookmarks" title="My News Feeds" 
			url="NewsReaderFeeds"/>
		<module Container="true" Display="normal" ORIGINALNAME="RSSReader/RSSReader" Output="true" ShowPrint="false" 
			id="rssReader1" location="right" maxItems="9999" moduleHREF="/Home/Modules//Accounts/catalog.xml#rssReader" 
			name="RSSReader/RSSReader" style="height:500px;overflow:auto;" targetID="" title="RSS Reader"
			rss="http://www.digg.com/rss/index.xml" />
	</modules>
	
	<eventListeners>
		<event eventHandler="rssReader1.getFeed" eventName="onClick" objectName="bookmarks21"/>
		<event eventHandler="rssReader1.getFeed" eventName="onClick" objectName="bookmarks22"/>
	</eventListeners>
	
</Page>

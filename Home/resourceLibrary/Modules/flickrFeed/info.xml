<?xml version="1.0" encoding="UTF-8"?>
<catalog package="Internet">

<modules>
	
	<!-- ********  Flickr Feed  ************ -->
	<module access="general" id="FlickrFeed" name="flickrFeed/flickrFeed">
		<description>
			Displays pictures from flickr. Pictures can be displayed by user or by tags.
		</description>
		<attributes>
			<attribute default="" description="Flickr Username" name="username" required="false"/>
			<attribute default="" description="Flickr User ID" name="userid" required="false"/>
			<attribute default="" description="Tags" name="tags" required="false"/>
			<attribute default="true" description="Show Header?" name="showheader" required="false"/>
			<attribute default="true" description="On Click Goto Flickr?" name="onClickGotoFlickr" required="false"/>
			<attribute default="" description="Maximum number of pictures to display" name="maxItems" required="false" />
		</attributes>
		<moduleInfo>
			<authorName>Oscar Arevalo</authorName>
			<authorEmail>oarevalo@gmail.com</authorEmail>
			<authorURL>http://www.homeportals.com</authorURL>
			<screenshot/>
		</moduleInfo>
	</module>
	
</modules>
</catalog>

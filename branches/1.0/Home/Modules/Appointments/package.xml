<?xml version="1.0" encoding="UTF-8"?>
<catalog>
<modules>
	<module access="general" id="Appointments" name="Appointments/appointments">
		<description>
			Maintains a list of appointments.
		</description>
		<resources>
			<resource href="/home/modules/Appointments/Appointments.js" type="script"/>
		</resources>		
		<attributes>
			<attribute description="[Optional] Indicates the URL of a document where to store your appointments. Leave empty to use default storage." name="calendarURL" required="true"/>
		</attributes>
		<eventListeners>
			<event eventHandler="$ID$.init" eventName="PageLoaded" objectName="Framework"/>
		</eventListeners>
	</module>	
</modules>
</catalog>

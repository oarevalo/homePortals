<?xml version="1.0" encoding="UTF-8"?>
<catalog>
<modules>
	<!-- ********   FlickrFeed  ************ -->
	<module access="general" id="FlickrFeed" name="flickrFeed/flickrFeed">
		<description>
			Displays a feed from flickr.
		</description>
		<attributes>
			<attribute default="" description="User ID" name="userid" required="false"/>
			<attribute default="" description="User ID List" name="useridlist" required="false"/>
			<attribute default="" description="Tags" name="tags" required="false"/>
			<attribute default="true" description="Show Header?" name="showheader" required="false"/>
			<attribute default="true" description="On Click Goto Flickr?" name="onClickGotoFlickr" required="false"/>
		</attributes>
	</module>
</modules>
</catalog>